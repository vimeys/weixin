<template name="shop_count">
    <block wx:for="{{Data}}" wx:for-item="item">
        <view class="list">
            <view class="img">
                <image src="../images/scan.png"></image>
            </view>
            <view class="table">
                <view class="title">{{item.goodsFashion}}
                    <text>{{item.type==0?"正常入库":"退货入库"}}</text>
                </view>
                <view>款号：
                    <text>{{item.goodsGirard}}</text>
                </view>
                <view>条码号：
                    <text>{{item.formatCode}}</text>
                </view>
                <view class="last">
                    尺码：
                    <text>{{item.sizeName}}</text>
                    颜色：
                    <text>{{item.colorName}}</text>
                    数量：
                    <text class="red">{{item.goodsStock}}</text>
                </view>
            </view>
        </view>
        <view class="record">
            <!--入库时间：2017-08-01 10:11:32-->
            入库时间：{{item.okTime}}
        </view>
    </block>
</template>