.all{
    width: 100%;
    display: flex;
    flex-direction: column;
    position: absolute;
    top:0;
    align-items: center;
}
.model{
    margin-top: 100rpx;
    background: #e5e5e5;
    padding: 20rpx;
    border-radius: 30rpx;
}
.model view{
    text-align: center;
    padding-bottom: 20rpx;
    border-bottom: 1px solid #333333;
}
.model label{
    display: block;
    text-align: left;
    margin-top: 20rpx;
}