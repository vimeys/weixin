<!--pages/shopIn_restockDetail/shopIn_restockDetail.wxml-->
<!--pages/shopIn_takeDetail/shopIN_takeDetail.wxml-->
<view class="num">
    订单号：{{order.orderCode}}
</view>
<block wx:for="{{Data}}" wx:for-item="item">
    <view class="list">
        <view class="img">
            <image src="../images/scan.png"></image>
        </view>
        <view class="table">
            <view class="title">{{item.goodsFashion}}</view>
            <view>款号：<text>{{item.goodsGirard}}</text></view>
            <view>条码号：<text>{{item.goodsCode}}</text></view>
            <view class="last">
                尺码：<text>{{item.sizeName}}</text>颜色：<text>{{item.colorName}}</text> 数量： <text class="{{item.fixnumber==0?'red':'del'}}">{{item.goodsStock}}</text><text   class="red" wx:if="{{item.fixnumber>0}}">{{item.fixnumber}}</text>
            </view>
            <image src="../images/pencil.png" data-type="{{item.storeId}}" bindtap="click"></image>
        </view>
    </view>
</block>
<view class="express">
    {{order.expressName}}：{{order.expressCode}}
</view>
<view class="btn">
    <view url="../shopIn_change/shopIn_change" class="change" bindtap="changeConfirm">发货修改</view>
    <view  class="confirm" bindtap="confirm">确认入库</view>
</view>