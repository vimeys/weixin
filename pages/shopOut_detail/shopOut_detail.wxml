<!--pages/shopOut_detail/shopOut_detail.wxml-->
<view class="det_title">待收货</view>
<view class="orderList">
    <view class="second">收货人：{{order.receiver}}<text>联系电话：{{order.rephone}}</text></view>
    <view class="third">店铺地址：{{order.address}}</view>
    <view class="fourth">发货人:：{{order.shipper}}<text>联系电话：{{order.shphone}}</text></view>
    <view class="small">发货时间：{{order.okTime}} </view>
</view>
<view class="detail">
    <view class="num">订单号：{{order.orderCode}}</view>
    <block wx:for="{{Data}}" wx:for-item="item">
        <view class="list">
            <view class="img">
                <image src="../images/scan.png"></image>
            </view>
            <view class="table">
                <view class="title">{{item.goodsFashion}}</view>
                <view>款号：<text>{{item.goodsGirard}}</text></view>
                <view>条码号：<text>{{item.goodsCode}}</text></view>
                <view class="last">
                    尺码：<text>{{item.sizeName}}</text>颜色：<text>{{item.colorName}}</text> 数量： <text class="red">{{item.goodsStock}}</text>
                </view>
            </view>
        </view>
    </block>



    <view class="express">{{order.expressName}}：{{item.expressCode}}</view>
</view>
<view class="fix"  bindtap="return">
    返回首页
</view>
