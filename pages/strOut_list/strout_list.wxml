            <view class="img">
                <image src="../images/scan.png"></image>
            </view>
            <view class="table">
                <view class="title">超级好看的鞋子</view>
                <view>款号：<text>45574543156</text></view>
                <view>条码号：<text>461646164164</text></view>
                <view class="last">
                    尺码：<text>L</text>颜色：<text>红色</text> 数量： <input type="number" value=""/>
                </view>
                <image src="../images/delete.png" bindtap="delThis" data-type="index"></image>
            </view>
        </view>
    </block>
    <view class="add"  bindtap="getMore">继续添加</view>
    <view class="fix" bindtap="express">确认并填写快递单</view>
</view>
