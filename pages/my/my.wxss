/* pages/my/my.wxss */
page{
    background-color: #eee;
}
.top{
    background-color: #1caba2;
    display: flex;
    flex-direction: column;
    justify-content: center;
}
.touxiang{
    display: flex;
    justify-content: center;
}
.touxiang image{
    width: 225rpx;
    height: 225rpx;
    border-radius: 50%;
}
.text{
    margin-top: 30rpx;
    padding-bottom: 40rpx;
    display: flex;
    justify-content: center;
    font-size: 30rpx;
    color: #fff;
}
.title-all{
    margin-top: 20rpx;
    font-size: 30rpx;
}
.title{
    padding-left: 40rpx;
    margin-top: 2rpx;
    display: flex;
    flex-direction: column;
    justify-content: center;
    line-height: 88rpx;
    background-color: #fff;
    color: #171717;
}