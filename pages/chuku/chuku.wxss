/* pages/chuku/chuku.wxss */
navigator{
    width: 300rpx;
    margin: 90rpx auto 0;
}
navigator image{
    width: 300rpx;
    height: 300rpx;
    margin: 0 auto;
}
.title{
    font-size: 40rpx;
    text-align: center;
    margin-top: 20rpx;
    margin-bottom: 50rpx;
}
.title_bottom{
    margin-bottom: 140rpx;
}

.all{
    width: 100%;
    display: flex;
    flex-direction: column;
    position: absolute;
    top:0;
    align-items: center;
}
.model{
    margin-top: 100rpx;
    background: #e5e5e5;
    padding: 20rpx;
    border-radius: 30rpx;
}
.model view{
    text-align: center;
    padding-bottom: 20rpx;
    border-bottom: 1px solid #333333;
}
.model label{
    display: block;
    text-align: left;
    margin-top: 20rpx;
}


















