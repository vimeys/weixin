.express{
    width: 100%;
    font-size: 24rpx;
    padding-top: 20rpx;
    padding-bottom: 20rpx;
    padding-right: 20rpx;
    box-sizing: border-box;
    text-align: right;
    border-bottom: 1rpx solid #ccc;
}
.reason{
    padding-left: 20rpx;
    padding-right: 20rpx;
    font-size: 24rpx;
    border-bottom: 1rpx solid #ccc;
    margin-bottom: 120rpx;
}
.return_reason{
    margin-top: 20rpx;
    text-align: left;
}
.return_time{
    margin-top: 70rpx;
    text-align: right;
    margin-bottom: 20rpx;
}
.det_title{
    width: 100%;
    text-align: center;
    padding-top: 40rpx;
    padding-bottom: 20rpx;
    font-size: 40rpx;
    border-bottom: 10rpx solid #e5e5e5;
}
.orderList{
    width: 100%;
    display: flex;
    flex-direction: column;
    /*align-items: center;*/
    font-size: 30rpx;
    border-bottom: 10rpx solid #e5e5e5;
}
