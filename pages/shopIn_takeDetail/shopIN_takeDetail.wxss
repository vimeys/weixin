.num{
    padding-left: 20rpx;
    font-size: 30rpx;
    margin-top: 30rpx;
    margin-bottom: 20rpx;
}
.list{
    border-top: 1rpx solid #ccc;
    width: 100%;
    border-bottom: 1rpx solid #ccc;
    display: flex;
    padding-top: 30rpx;
}
.img{
    padding-left: 20rpx;
    width: 270rpx;
    flex: initial;
}
image{
    width:250rpx;
    height: 178rpx;
}
.table{
    padding-top: 10rpx;
    padding-bottom: 10rpx;
    flex:1;
    font-size: 24rpx;
    line-height:44rpx;
    position: relative;
}
.table .title{
    font-size: 28rpx;
    margin-bottom: 10rpx;

}
.table input{
    display: inline-block;
    border: 1rpx solid #333333;
    width: 80rpx;
    height: 38rpx;
    color:#FF0202;
    vertical-align: middle;
    font-size: 28rpx;
    min-height: 38rpx;
    padding-left: 5rpx;
    box-sizing: border-box;
}
.table .last{
    line-height: 44rpx;
}
.table .last text{
    margin-right: 15rpx;
}
.del{
    color:#cccccc;
    text-decoration:line-through;
}
.table .last .red{
    color:red;
}
.table image{
    width: 30rpx;
    height: 30rpx;
    position: absolute;
    top:10rpx;
    right: 20rpx;
}
